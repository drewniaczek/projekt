// KlasaA.java
package pakiet1;

public class KlasaA {
    private int liczba;

    public KlasaA(int liczba) {
        this.liczba = liczba;
    }

    public int getLiczba() {
        return liczba;
    }

    @Override
    public boolean equals(Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj == null || getClass() != obj.getClass()) {
            return false;
        }
        KlasaA other = (KlasaA) obj;
        return liczba == other.liczba;
    }
}